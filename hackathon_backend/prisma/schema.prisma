model Keyword {
  id      Int     @id @unique(map: "Keyword_id_uindex") @default(autoincrement())
  keyword String? @db.VarChar(50)
  userId  Int?
  User    User?   @relation(fields: [userId], references: [userId], onDelete: NoAction, onUpdate: NoAction, map: "Keyword_User_userId_fk")

  @@index([userId], map: "Keyword_User_userId_fk")
}

model User {
  userId  Int       @id @default(autoincrement())
  email   String?   @db.VarChar(30)
  Keyword Keyword[]
}
